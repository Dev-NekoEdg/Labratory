{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "laboratory": "[stringConnection]"
  },
  "CommonConfig": {
    "AllowedImageExt": [ "bmp", "jpeg", "jpg", "png", "webp" ]
  },
  "BlobStorageConfig": {
    "Url": "[blobstorageKey]",
    "ContainerName": "[containerName]",
    "CommonMIMETypes": {
      "bmp": "image/bmp",
      "gif": "image/gif",
      "txt": "text/plain",
      "ico": "image/vnd.microsoft.icon",
      "jpeg": "image/jpeg",
      "jpg": "image/jpeg",
      "json": "application/json",
      "mp3": "audio/mpeg",
      "mp4": "video/mp4",
      "mpeg": "video/mpeg",
      "png": "image/png",
      "pdf": "application/pdf",
      "webp": "image/webp",
      "default": "application/octet-stream"
    }
  }

}
